{
  "name": "gulp-sass-monster",
  "version": "0.0.1",
  "description": "Gulp plugin for node-sass with steroids. The project is inspired by gulp-sass",
  "main": "index.js",
  "scripts": {
    "render-node": "node ./tests/node-sass.js",
    "render-gulp": "cd tests && gulp sass",
    "compare": "cd tests && gulp compare-results",
    "render": "npm run render-node && npm run render-gulp && npm run compare",
    "happiness": "happiness \"./*.js\" \"./utils/**/*.js\" --verbose --fix | snazzy",
    "test": "npm run happiness && npm run render"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dutchenkoOleg/gulp-sass-monster.git"
  },
  "engines": {
    "node": ">=6.3.1"
  },
  "keywords": [],
  "author": "Oleg Dutchenko <dutchenko.o.dev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dutchenkoOleg/gulp-sass-monster/issues"
  },
  "homepage": "https://github.com/dutchenkoOleg/gulp-sass-monster#readme",
  "dependencies": {
    "chalk": "^2.1.0",
    "gulp-not-supported-file": "^1.2.0",
    "gulp-util": "^3.0.8",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "through2": "^2.0.3",
    "vinyl-sourcemaps-apply": "^0.2.1"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "del": "^3.0.0",
    "glob": "^7.1.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-if": "^2.0.2",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-mocha": "^4.3.1",
    "gulp-sourcemaps": "^2.6.1",
    "happiness": "^10.0.2",
    "jsdoc-simple-theme": "^1.1.0",
    "snazzy": "^7.0.0",
    "yargs": "^8.0.2"
  }
}
